{
  "name": "Gmail Hide Unread",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "description": "Hide the unread count count in the gmail tab title.",
  "content_scripts": [{
    "matches": ["*://mail.google.com/*", "*://www.facebook.com/*", "*://twitter.com/*"],
    "js": ["hide_count.js"]
  }],
  "permissions": [
    "tabs"
  ]
}
